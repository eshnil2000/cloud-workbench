<% description = 'WorkBench 500 - Internal server error' %>
<% provide(:title, description) %>
<% content_header(description) %>

<div class="error-page">
  <h2 class="headline">500</h2>
  <div class="error-content">
    <h3><i class="fa fa-warning text-yellow"></i> Oops! Something went wrong.</h3>
    <p>
      Meanwhile, you may <%= link_to 'return to dashboard', root_path %>
    </p>
  </div>
</div>
